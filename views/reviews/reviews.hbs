
    <h1>{{username}} All Reviews</h1>
    <ul>
        {{#each reviews}}
        <li>
            <h1>{{this.author}}</h1>
            <h2>{{this.content}}</h2>

            
        <form action="/games/{{_id}}/delete" method="post">
            <button type="submit">Remove</button>
        </form>

        </li>
        {{/each}}
        
    </ul>
    <a href="/">Home</a>
