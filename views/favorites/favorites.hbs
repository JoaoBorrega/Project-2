
    <h1>{{usertitle}} Favorite Games</h1>
    <ul>
        {{#each favorites}}
        <li>
            <h1>{{this.title}}</h1>
            <img src="/images/{{this.image}}">

        <form action="/removeFavorites/{{this._id}}" method="post">
            <button type="submit">Remove</button>
        </form>

        </li>
        <hr>
        {{/each}}
        
    </ul>
    <a href="/">Home</a>
    <a href="/games">Games List</a>
